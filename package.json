{
  "name": "starkpil",
  "version": "0.0.1",
  "description": "Build a stark from a PIL",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "fibonacci_buidconst": "node test/sm_fibonacci/main_genconst_fibonacci.js -o tmp/fibonacci.const.bin",
    "fibonacci_exec": "node test/sm_fibonacci/main_exec_fibonacci.js -i test/sm_fibonacci/fibonacci.input.json -o tmp/fibonacci.commit.bin",
    "fibonacci_buildconsttree": "node src/main_buildconsttree.js -c tmp/fibonacci.const.bin -p test/sm_fibonacci/fibonacci.pil -s tmp/fibonacci.starkstruct.json -t tmp/fibonacci.consttree.bin -v tmp/fibonacci.verkey.json",
    "fibonacci_compileverifier": "circom --O1 --prime goldilocks --r1cs --sym --wasm --verbose tmp/fibonacci.verifier.circom -o tmp"
  },
  "keywords": [
    "stark",
    "pil",
    "zksnark",
    "prover",
    "polygon",
    "hermez"
  ],
  "dependencies": {
    "circom_runtime": "^0.1.18",
    "circomlibjs": "^0.1.4",
    "ejs": "^3.1.8",
    "tmp-promise": "^3.0.3",
    "zkpil": "git+ssh://git@github.com/hermeznetwork/zkpil.git"
  },
  "author": "Jordi Baylina",
  "license": "GPL-3.0",
  "devDependencies": {
    "@iden3/bigarray": "^0.0.2",
    "chai": "^4.3.6",
    "circom_tester": "^0.0.14",
    "circomlib": "^2.0.5",
    "json-bigint": "^1.0.0",
    "mocha": "^10.0.0",
    "r1csfile": "^0.0.38"
  }
}
