    function ref(r) {
        if (r.type == "eval") {
            return "evals[" + r.id + "]";
        } else if (r.type == "challange") {
            return "challanges[" + r.id + "]";
        } else if (r.type == "public") {
            return "publics[" + r.id + "]";
        } else if (r.type == "x") {
            return "challange[7]";
        } else if (r.type == "Z") {
            return "Z";
        } else if (r.type == "xDivXSubXi") {
            return "xDivXSubXi";
        } else if (r.type == "xDivXSubWXi") {
            return "xDivXSubWXi";
        } else if (r.type == "tmp") {
            return "tmp_"+r.id;
        } else if (r.type == "tree1") {
            return "tree1[" + r.id + "]";
        } else if (r.type == "tree2") {
            return "tree2[" + r.id + "]";
        } else if (r.type == "tree3") {
            return "tree3[" + r.id + "]";
        } else if (r.type == "tree4") {
            return "tree4[" + r.id + "]";
        } else if (r.type == "const") {
            return "consts[" + r.id + "]";
        } else if (r.type == "number") {
            return r.value;
        } else {
            throw new Error("Invalid Ref: " +r.type);
        }
    }
