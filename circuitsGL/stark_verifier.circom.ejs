        s0_merkle3[q] = MerkleHash(3, <%- starkInfo.nCm3 + starkInfo.nQ3 %>, <%- 1 << starkStruct.steps[0].nBits %>);
<% }                                         %>
        s0_merkle4[q] = MerkleHash(3, <%- starkInfo.nCm4 + starkInfo.nQ4 %>, <%- 1 << starkStruct.steps[0].nBits %>);
        s0_merkleC[q] = MerkleHash(1, <%- starkInfo.nConst %>, <%- 1 << starkStruct.steps[0].nBits %>);
        s0_lowValues[q] = TreeSelector(<%- starkStruct.steps[0].nBits - ((0< starkStruct.steps.length-1) ? starkStruct.steps[1].nBits : 0)  %>, 3) ;

        for (var i=0; i<<%- starkStruct.steps[0].nBits %>; i++ ) {
            verifyQueries[q].ys[i] <== ys[q][i];
            s0_merkle1[q].key[i] <== ys[q][i];
