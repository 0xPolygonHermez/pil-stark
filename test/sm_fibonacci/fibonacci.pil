
constant %N = 2**10;

namespace Fibonacci(%N);


    pol bool constant L1, LLAST;
    pol commit l1,l2;

    public in1 = l2(0);
    public in2 = l1(0);
    public out = l1(%N-1);

    (l2' - l1)*(1-LLAST) = 0;

    pol next = l1*l1 + l2*l2;
    
    (l1' - next)*(1-LLAST) = 0;

    L1 * (l2 - :in1) = 0;
    L1 * (l1 - :in2) = 0;
    LLAST * (l1 - :out) = 0;

